    {
      "cell_type": "code",
      "source": [
        "def create_user_movie_df()\n",
        "  import pandas as pd\n",
        "\n",
        "  movie = pd.read_csv(\"\")\n",
        "  rating = pd.read_csv(\"\")\n",
        "  df = movie.merge(rating, how=\"left\", on=\"movieId\")\n",
        "\n",
        "  comment_counts = pd.DataFrame(df[\"title\"].value_counts()).index\n",
        "  common_movie = df[~df[\"title\"] < 1000].isin(comment_counts)\n",
        "\n",
        "  user_movie_df = common_movie.pivot_table(index=[\"userId\"], columns=[\"title\"], values=\"rating\")\n",
        "\n",
        "  return user_movie_df"
      ],
      "metadata": {
